# 데이터베이스 설정
POSTGRES_DB=trading_journal
POSTGRES_USER=trading_user
POSTGRES_PASSWORD=trading_password
DATABASE_URL=postgresql://trading_user:trading_password@localhost:5432/trading_journal

# Redis 설정
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379

# 백엔드 API 설정
BACKEND_URL=http://localhost:8080
SPRING_PROFILES_ACTIVE=dev

# 매매평가 엔진 설정
TRADING_ENGINE_URL=http://localhost:8000
PYTHONPATH=/app

# 프론트엔드 설정
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_TRADING_ENGINE_URL=http://localhost:8000
NODE_ENV=development

# JWT 시크릿 키 (프로덕션에서는 반드시 변경)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# 외부 API 키들 (필수: GPT 패턴 분석용)
OPENAI_API_KEY=your-openai-api-key

# 외부 API 키들 (선택)
BINANCE_API_KEY=your-binance-api-key
BINANCE_SECRET_KEY=your-binance-secret-key

# 로그 레벨
LOG_LEVEL=debug

# 도커 네트워크 설정
DOCKER_NETWORK_SUBNET=172.20.0.0/16

